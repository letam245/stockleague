
<div id="burgerImageRow" class="row"> 
<div class="col-md-4"></div>  
<div class="col-md-4">   
<h1 id="mainHeader">Burgers!</h1>
<img src="../assets/img/burger.png" alt="burger" style="width:304px;height:228px;">
</div>
<div class="col-md-4">
    <script ...><{{test}}/script>
</div>
</div>    

<div id="burgersRow" row>
    <div class="col-md-6">  
<div id="notDevouredBlock"> 
<h2 id="notDevouredTitle">Burgers that are NOT devoured!</h2>

<ol>
  {{#each burgers}}
    {{#unless this.devoured}}
      <li>
        {{this.burger_name}}

        <form action="/{{this.id}}?_method=PUT" method="POST">
          <input type="hidden" name="devoured" value="true">
          <button type="submit">Devour it!</button>
        </form>
      </li>
    {{/unless}}
  {{/each}}
</ol>

</div>

</div>
